{
  "name": "frontend",
  "version": "4.3.26",
  "scripts": {
    "serve": "ng serve --proxy-config src/proxy-dev.conf.js",
    "serve:dev:nl": "ng serve --configuration=nl --proxy-config src/proxy-dev.conf.js",
    "serve:dev:de": "ng serve --configuration=de --proxy-config src/proxy-dev.conf.js",
    "serve:dev:fr": "ng serve --configuration=fr --proxy-config src/proxy-dev.conf.js",
    "serve:translation:nl": "ng serve --configuration=nl --proxy-config src/proxy-translation.conf.js",
    "serve:translation:de": "ng serve --configuration=de --proxy-config src/proxy-translation.conf.js",
    "serve:translation:fr": "ng serve --configuration=fr --proxy-config src/proxy-translation.conf.js",
    "extract-i18n": "ng extract-i18n --output-path src/locale",
    "prettier-check": "prettier --check --config .prettierrc --ignore-path .prettierignore --log-level warn \"{**/*,*}.{js,ts}\"",
    "prettier-write": "prettier --write --config .prettierrc --ignore-path .prettierignore --log-level warn \"{**/*,*}.{js,ts}\"",
    "lint": "ng lint",
    "build": "ng build",
    "test": "ng test",
    "build-prod": "rm -rf ./dist/*; ng run frontend:build:production --build-optimizer --progress --source-map --output-path ./dist/${npm_package_name}-${npm_package_version};cd dist/${npm_package_name}-${npm_package_version}; gzip --keep */*.js */*.json */*.css",
    "zip": "cd dist; tar -czf ../../dist/${npm_package_name}-${npm_package_version}.tar.gz .",
    "dist": "run-s build-prod zip"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.4",
    "@angular/cdk": "17.3.3",
    "@angular/common": "^17.3.4",
    "@angular/compiler": "^17.3.4",
    "@angular/core": "^17.3.4",
    "@angular/forms": "^17.3.4",
    "@angular/localize": "17.3.4",
    "@angular/material": "17.3.3",
    "@angular/platform-browser": "^17.3.4",
    "@angular/platform-browser-dynamic": "^17.3.4",
    "@angular/router": "^17.3.4",
    "@sentry/angular-ivy": "^7.109.0",
    "@swimlane/ngx-charts": "^20.5.0",
    "@types/file-saver": "^2.0.7",
    "angular-oauth2-oidc": "^15.0.1",
    "canvg": "^4.0.1",
    "file-saver-es": "^2.0.5",
    "immutable": "^5.0.0-beta.5",
    "jspdf": "^2.5.1",
    "marked": "^9.1.6",
    "ngx-clipboard": "^16.0.0",
    "ngx-cookie-service": "^17.1.0",
    "ngx-markdown": "17.2.0",
    "ngxtension": "^2.2.1",
    "ol": "9.1.0",
    "ol-geocoder": "4.3.3",
    "ol-mapbox-style": "12.2.1",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.3",
    "@angular-devkit/core": "17.3.3",
    "@angular-devkit/schematics": "17.3.3",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "^17.3.3",
    "@angular/compiler-cli": "^17.3.4",
    "@angular/language-service": "17.3.4",
    "@schematics/angular": "17.3.3",
    "@softarc/eslint-plugin-sheriff": "^0.14.5",
    "@softarc/sheriff-core": "^0.14.5",
    "@types/jasmine": "~5.1.4",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "svg-inline-loader": "^0.8.2",
    "ts-node": "~10.9.2",
    "typescript": "5.4.3"
  }
}
